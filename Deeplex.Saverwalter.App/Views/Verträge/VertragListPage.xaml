<Page
    x:Class="Deeplex.Saverwalter.App.Views.VertragListPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:uc="using:Deeplex.Saverwalter.App.UserControls"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <uc:VertragListControl
        OnlyActive="{x:Bind OnlyActive.Value, Mode=OneWay}"
        Filter="{x:Bind ViewModel.Filter.Value, Mode=OneWay}"/>
</Page>
