<Page
    x:Class="Deeplex.Saverwalter.WinUI3.Views.Rechnungen.BetriebskostenrechnungPrintPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    xmlns:conv="using:Deeplex.Saverwalter.WinUI3.Utils"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Page.Resources>
        <conv:GetBriefAnredeConverter x:Key="GetBriefAnredeConverterKey" />
        <conv:DateStringConverter x:Key="DateStringConverterKey" />
        <conv:ToDescriptionStringConverter x:Key="ToDescriptionStringConverterKey" />
        <conv:ToStringConverter x:Key="ToStringConverterKey" />
    </Page.Resources>

    <ScrollViewer
        HorizontalScrollBarVisibility="Visible"
        IsScrollInertiaEnabled="True"
        HorizontalScrollMode="Auto"
        Background="{StaticResource SystemChromeHighColor}">
        <StackPanel
            Margin="10"
            Padding="101.19, 68.404, 40.476, 68.404" x:Name="Panel" Width="850" Background="{StaticResource SystemAltHighColor}" />
    </ScrollViewer>
</Page>
